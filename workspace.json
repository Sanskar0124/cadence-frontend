		"salesforce": {
			"root": "apps/salesforce",
			"sourceRoot": "apps/salesforce/src",
			"projectType": "application",
			"targets": {
				"build": {
					"executor": "@nrwl/web:build",
					"outputs": ["{options.outputPath}"],
					"options": {
						"outputPath": "dist/apps/salesforce",
						"index": "apps/salesforce/src/index.html",
						"main": "apps/salesforce/src/main.js",
						"polyfills": "apps/salesforce/src/polyfills.js",
						"tsConfig": "apps/salesforce/tsconfig.app.json",
						"assets": ["apps/salesforce/src/favicon.ico", "apps/salesforce/src/assets"],
						"styles": ["apps/salesforce/src/styles.scss"],
						"scripts": [],
						"webpackConfig": "@nrwl/react/plugins/webpack",
						"extractCss": true
					},
					"configurations": {
						"production": {
							"fileReplacements": [
								{
									"replace": "libs/environments/src/environments/environment.js",
									"with": "libs/environments/src/environments/environment.prod.js"
								}
							],
							"optimization": true,
							"outputHashing": "all",
							"sourceMap": false,
							"extractCss": true,
							"namedChunks": false,
							"extractLicenses": true,
							"vendorChunk": false
						},
						"staging": {
							"fileReplacements": [
								{
									"replace": "libs/environments/src/environments/environment.js",
									"with": "libs/environments/src/environments/environment.staging.js"
								}
							],
							"optimization": true,
							"outputHashing": "all",
							"sourceMap": false,
							"extractCss": true,
							"namedChunks": false,
							"extractLicenses": true,
							"vendorChunk": false,
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "2mb",
									"maximumError": "30mb"
								}
							]
						},
						"dev": {
							"fileReplacements": [
								{
									"replace": "libs/environments/src/environments/environment.js",
									"with": "libs/environments/src/environments/environment.dev.js"
								}
							],
							"optimization": true,
							"outputHashing": "all",
							"sourceMap": false,
							"extractCss": true,
							"namedChunks": false,
							"extractLicenses": true,
							"vendorChunk": false,
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "2mb",
									"maximumError": "30mb"
								}
							]
						},
						"dev2": {
							"fileReplacements": [
								{
									"replace": "libs/environments/src/environments/environment.js",
									"with": "libs/environments/src/environments/environment.dev2.js"
								}
							],
							"optimization": true,
							"outputHashing": "all",
							"sourceMap": false,
							"extractCss": true,
							"namedChunks": false,
							"extractLicenses": true,
							"vendorChunk": false,
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "2mb",
									"maximumError": "30mb"
								}
							]
						},
						"dev3": {
							"fileReplacements": [
								{
									"replace": "libs/environments/src/environments/environment.js",
									"with": "libs/environments/src/environments/environment.dev3.js"
								}
							],
							"optimization": true,
							"outputHashing": "all",
							"sourceMap": false,
							"extractCss": true,
							"namedChunks": false,
							"extractLicenses": true,
							"vendorChunk": false,
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "2mb",
									"maximumError": "30mb"
								}
							]
						}
					}
				},
				"serve": {
					"executor": "@nrwl/web:dev-server",
					"options": {
						"buildTarget": "salesforce:build",
						"hmr": true
					},
					"configurations": {
						"production": {
							"buildTarget": "salesforce:build:production",
							"hmr": false
						}
					}
				},
				"lint": {
					"executor": "@nrwl/linter:eslint",
					"options": {
						"lintFilePatterns": ["apps/salesforce/**/*.{ts,tsx,js,jsx}"]
					}
				},
				"test": {
					"executor": "@nrwl/jest:jest",
					"outputs": ["coverage/apps/salesforce"],
					"options": {
						"jestConfig": "apps/salesforce/jest.config.js",
						"passWithNoTests": true
					}
				}
