{
	"version": 2,
	"cli": {
		"defaultCollection": "@nrwl/react"
	},
	"defaultProject": "salesforce",
	"generators": {
		"@nrwl/react": {
			"application": {
				"style": "scss",
				"linter": "eslint",
				"strict": true,
				"babel": true
